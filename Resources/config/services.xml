<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="gubler.uuid_encoder.default_encoder" class="Gubler\UuidEncoder\UuidEncoder">
            <argument></argument>
        </service>
        <service id="gubler.uuid_encoder.url_encoder" class="Gubler\UuidEncoderBundle\Util\UrlUuidEncoder">
            <argument></argument>
        </service>
        <service id="gubler.uuid_encoder.filesystem_encoder" class="Gubler\UuidEncoderBundle\Util\FilesystemUuidEncoder">
            <argument></argument>
        </service>
    </services>
</container>
